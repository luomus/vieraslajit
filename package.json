{
  "name": "vieraslajit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "webpack-high-memory": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js",
    "ng": "ng",
    "update": "ng update @angular/cli @angular/core",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc:build": "compodoc -p src/tsconfig.app.json",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json -s",
    "compile:server": "yarn run webpack-high-memory --config webpack.server.config.js --progress --colors",
    "ssr:run:local": "yarn build:ssr:local && yarn serve:ssr",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr:local": "npm run build:client-and-server-bundles:local && npm run compile:server",
    "build:client-and-server-bundles": "yarn run ng-high-memory build --prod --base-href https://vieraslajit-dev.laji.fi/ && yarn run ng-high-memory run vieraslajit:server:production",
    "build:client-and-server-bundles:local": "yarn run ng-high-memory build --prod --base-href http://localhost:4000/ && yarn run ng-high-memory run vieraslajit:server:production",
    "bundle-report": "ng build --prod --aot --stats-json --named-chunks=true && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/cli": "^7.3.6",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/platform-server": "^7.2.10",
    "@angular/pwa": "^0.13.3",
    "@angular/router": "^7.2.10",
    "@angular/service-worker": "^7.2.10",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-datatable": "git+https://git@github.com/vixriihi/laji-datatable",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "domino": "^2.1.3",
    "express": "^4.16.3",
    "fast-deep-equal": "^2.0.1",
    "jquery": "^3.3.1",
    "laji-form": "^0.0.611",
    "laji-map": "1.0.494",
    "ngx-bootstrap": "^3.0.1",
    "ngx-infinite-scroll": "^7.0.1",
    "ngx-pagination": "^3.2.0",
    "npm": "^6.3.0",
    "open-iconic": "^1.1.1",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@compodoc/compodoc": "^1.0.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/jquery": "^3.3.5",
    "@types/node": "^10.5.7",
    "codelyzer": "^4.4.3",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.2.4",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.1.0"
  },
  "resolutions": {
    "leaflet": "1.3.4"
  }
}
