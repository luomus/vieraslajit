    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-datatable": "^17.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "express": "4.17.2",
    "fast-deep-equal": "^2.0.1",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^6.0.0",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-pagination": "^3.2.0",
    "npm": "^6.3.0",
