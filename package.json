{
  "name": "vieraslajit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "update": "ng update @angular/cli @angular/core",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --prod --aot --stats-json --named-chunks=true && webpack-bundle-analyzer dist/browser/stats.json",
    "serve:ssr": "node dist/server/main.js",
    "dev:ssr": "ng run vieraslajit:serve-ssr",
    "build:local": "ng build --prod --configuration=beta --base-href=http://localhost:4000/ && ng run vieraslajit:server:beta",
    "build:beta": "ng build --prod --configuration=beta --base-href=https://vieraslajit-dev.laji.fi/ && ng run vieraslajit:server:beta",
    "build:prod": "ng build --prod --configuration=production --base-href=https://vieraslajit.fi/ && ng run vieraslajit:server:production",
    "prerender": "ng run vieraslajit:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "10.2.7",
    "@angular/cli": "^10.2.4",
