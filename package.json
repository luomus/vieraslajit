    "@types/node": "^10.5.7",
    "codelyzer": "^5.0.0",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
